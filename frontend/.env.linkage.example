# Frontend Environment Variables for Linkage VA Hub Deployment
# Deploy URL: https://linkage-va-hub.onrender.com

# API Configuration
REACT_APP_API_URL=https://linkage-va-hub.onrender.com/api

# Brand Configuration (determines UI/UX flow)
REACT_APP_BRAND=linkage

# LinkedIn OAuth Configuration (must match backend)
REACT_APP_LINKEDIN_CLIENT_ID=your_linkedin_app_client_id_here

# Application Name
REACT_APP_NAME=Linkage VA Hub

# Stripe Public Key (optional)
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# VideoSDK Configuration
REACT_APP_VIDEOSDK_API_KEY=your_videosdk_api_key_here

# Google Analytics (optional)
REACT_APP_GA_TRACKING_ID=your_google_analytics_id_here

# Environment
REACT_APP_ENV=production

# WebSocket URL (for real-time features)
REACT_APP_WS_URL=wss://linkage-va-hub.onrender.com

# Feature Flags
REACT_APP_ENABLE_LINKEDIN_AUTH=true
REACT_APP_ENABLE_VIDEO_CALLS=true
REACT_APP_ENABLE_PAYMENTS=false

# Support Email
REACT_APP_SUPPORT_EMAIL=support@linkage-va-hub.com

# Terms and Privacy URLs
REACT_APP_TERMS_URL=https://linkage-va-hub.com/terms
REACT_APP_PRIVACY_URL=https://linkage-va-hub.com/privacy

# Social Media Links (optional)
REACT_APP_FACEBOOK_URL=https://facebook.com/linkagevahub
REACT_APP_TWITTER_URL=https://twitter.com/linkagevahub
REACT_APP_LINKEDIN_URL=https://linkedin.com/company/linkage-va-hub

# Cloudinary Configuration (for image uploads)
REACT_APP_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
REACT_APP_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Sentry Error Tracking (optional)
REACT_APP_SENTRY_DSN=your_sentry_dsn_here